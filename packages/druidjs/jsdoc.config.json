{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": "src",
        "exclude": [".vscode", "data\\", "dist", "doc", "node_modules", "test", "*.bak"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "readme": "./README.md",
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        }
    }
}