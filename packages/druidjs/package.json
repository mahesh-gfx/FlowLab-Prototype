{
  "name": "@data-viz-tool/druidjs",
  "version": "0.7.3",
  "description": "A JavaScript Library for Dimensionality Reduction.",
  "keywords": [
    "dimensionality reduction",
    "matrix",
    "linear algebra"
  ],
  "homepage": "https://renecutura.eu",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rene Cutura",
    "url": "http://renecutura.eu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saehm/druidjs.git"
  },
  "type": "module",
  "files": [
    "src/**/*.js",
    "dist/**/*.js",
    "dist/**/*.cjs"
  ],
  "main": "dist/druid.cjs",
  "exports": {
    "require": "./dist/druid.cjs",
    "import": "./src/index.js"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "dev": "rollup -cw",
    "test": "mocha",
    "docs": "jsdoc -r -d docs -c jsdoc.config.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "clean-jsdoc-theme": "^4.2.9",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^2.38.3",
    "rollup-plugin-jsdoc": "^0.1.2"
  },
  "engines": {
    "node": ">=14"
  }
}
